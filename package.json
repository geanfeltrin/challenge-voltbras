{
  "name": "challenge-voltbras",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/geanfeltrin/challenge-voltbras.git",
  "author": "Gean Feltrin <geanfeltrin75@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run generate && tsc --skipLibCheck",
    "create:db": "prisma migrate save --create-db --experimental",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "migrate:down": "prisma migrate down --experimental",
    "generate": "npm -s run generate:prisma",
    "generate:prisma": "prisma generate",
    "dev": "ts-node-dev --inspect --ignore-watch node_modules src/server.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.10.2",
    "apollo-datasource-rest": "^0.9.5",
    "apollo-server": "^2.19.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.10.2",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
