  "author": "Gean Feltrin <geanfeltrin75@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run && tsc",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly ./src/server.ts",
    "dev:server": "ts-node-dev --inspect --ignore-watch node_modules src/server.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.10.2",
    "apollo-datasource-rest": "^0.9.5",
    "apollo-server": "^2.19.0",
    "graphql": "^15.4.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.10.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
